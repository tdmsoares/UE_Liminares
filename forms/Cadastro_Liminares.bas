Private Sub VerificarCiclo()
    '
    'Atribui o ciclo conforme data de nascimento
    Dim CódigoCiclo
    '
    Me.IdCiclo.Visible = True
    Me.IdCiclo.SetFocus
    Me.IdCiclo.Locked = False
    '
    CódigoCiclo = Verificar.DeterminarCiclo(Me.Nascimento, "Código")
    Me.IdCiclo = CódigoCiclo
    '
    Me.IdCiclo.Locked = True
    Me.Processo.SetFocus
    Me.IdCiclo.Visible = False
End Sub

Private Sub AoAlterar()
Alteração.Dados Me.name, Me.Nome.name, Me.Nome, Me.Código
Alteração.Dados Me.name, Me.Nascimento.name, Me.Nascimento, Me.Código
Alteração.Dados Me.name, Me.Processo.name, Me.Processo, Me.Código
Alteração.Dados Me.name, Me.IdCiclo.name, Me.IdCiclo, Me.Código
Alteração.Dados Me.name, Me.Ciclo.name, Me.Ciclo, Me.Código
