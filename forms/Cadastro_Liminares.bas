Private Sub Comando10_Click()
If (Not IsNull(Me.Código)) Then
    '
    'Verifica se algum campo não foi preenchido
    If (IsNull(Me.Nome)) Or Me.Nome = "" Then
        MsgBox "Nome da Criança não preenchido", vbExclamation, "Cadastro Liminar"
        Me.Nome.SetFocus
        End
    End If
    If (IsNull(Me.Nascimento)) Or Me.Nascimento = "" Then
        MsgBox "Nascimento da Criança não preenchido", vbExclamation, "Cadastro Liminar"
        Me.Nascimento.SetFocus
        End
    End If
    If (IsNull(Me.Processo)) Or Me.Processo = "" Then
        MsgBox "Processo referente a Criança não preenchido", vbExclamation, "Cadastro Liminar"
        Me.Processo.SetFocus
        End
    End If
    '
    'Pergunta ao usuário se deseja salvar
    Dim QueroCadastrar As VbMsgBoxResult
    QueroCadastrar = MsgBox("Deseja Cadastrar os dados referentes a liminar de: " & vbCrLf & Me.Nome & vbCrLf & "Nasc: " & Me.Nascimento & vbCrLf & "Processo: " & Me.Processo, vbYesNo + vbQuestion, "Cadastro Liminares")
    '
    If QueroCadastrar = vbYes Then
        '
        'Salva e abre o form para cadastrar a ocorrência da liminar
        DoCmd.RunCommand acCmdSaveRecord
        AoAlterar
        DoCmd.OpenForm ("Cadastrar_Acompanhamento")
        '
        '=========================================================================
        If (StatusLiminar(Me.Código)) = "Pendente" Then
            Form_Cadastrar_Acompanhamento.Ocorrência.RowSource = "Disponibilizada"
            Form_Cadastrar_Acompanhamento.Ocorrência.Value = "Disponibilizada"
            Form_Cadastrar_Acompanhamento.Ocorrência.Locked = True
        End If
        '
        Form_Cadastrar_Acompanhamento.IdCriança = Me.Código
        '=========================================================================
    End If
Else
    MsgBox "Dados da Criança não preenchidos", vbExclamation, "Cadastro Liminares"
End If
End Sub

