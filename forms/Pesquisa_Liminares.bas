Private Sub cmdOfício_Click()
'
Dim queroOfício As VbMsgBoxResult
'
If (Me.cmdOfício.Caption = ofícioNãoMatriculado) Then
    queroOfício = MsgBox("Será gerado Ofício em PDF/Doc informando que a família ainda não compareceu para Realizar a Matrícula pela Liminar" & vbCrLf & _
                        "Deseja continuar?", vbYesNo + vbQuestion, "Ofício Liminar")
    '
    If (queroOfício = vbYes) Then
        Ofício.NãoMatriculado Me.Código, True, True
    End If
ElseIf (Me.cmdOfício.Caption = ofícioDesistente) Then
    queroOfício = MsgBox("Será gerado Ofício em PDF/Doc informando da Desistência da família em relação ao Atendimento em Liminar" & vbCrLf & _
                        "Deseja continuar?", vbYesNo + vbQuestion, "Ofício Liminar")
    '
    If (queroOfício = vbYes) Then
        Ofício.Desistência Me.Código, True, True
    End If
End If
'
End Sub

Private Sub Comando109_DblClick(Cancel As Integer)
'
'Abre o form para Edição
DoCmd.OpenForm "Cadastro_Editar Liminares", , , "Código=" & Me.Código
End Sub

Private Sub Comando111_Click()
'
'Copia o Nome para a Area de Transferência
Me.Nome.Visible = True
Me.Nome.Enabled = True
Me.Nome.SetFocus
Me.Nome.Application.DoCmd.RunCommand acCmdCopy
Me.Nascimento.SetFocus
Me.Nome.Visible = False
Me.Nome.Enabled = False
End Sub

Private Sub Comando21_Click()
'
'Fecha e abre o Formulário
DoCmd.Close
DoCmd.OpenForm ("Menu_Liminares")
End Sub

Private Sub Form_Current()
'
'Atribui o cabeçalho com o nome da criança
Me.Auto_Title0.Caption = Me.Nome
'
'Atribui o botão de Ofício de acordo com o status da Liminar
If (Me.Status = "Aguardo") Then
    Me.cmdOfício.Enabled = True
    Me.cmdOfício.Visible = True
    Me.cmdOfício.Caption = ofícioNãoMatriculado
ElseIf (Me.Status = "Atendida") Then
    Me.cmdOfício.Enabled = True
    Me.cmdOfício.Visible = True
    Me.cmdOfício.Caption = ofícioDesistente
Else:
    Me.cmdOfício.Caption = ""
    Me.cmdOfício.Enabled = False
    Me.cmdOfício.Visible = False
End If
'
End Sub

Private Sub Nome_AfterUpdate()
'
'Deixa em Maiúsculo
Me.Nome = UCase(Me.Nome)
End Sub
